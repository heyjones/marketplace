<div id="newVendor" class="modal fade">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title">New Vendor</h4>
			</div>
			<form class="form-horizontal">
				<div class="modal-body">
					<div class="form-group">
						<label for="vendorName" class="col-sm-2 control-label">Vendor</label>
						<div class="col-sm-10">
							<input type="text" name="name" class="form-control" id="vendorName" placeholder="Vendor">
						</div>
					</div>
					<div class="form-group">
						<label for="vendorEmail" class="col-sm-2 control-label">Email</label>
						<div class="col-sm-10">
							<input type="email" class="form-control" name="email" id="vendorEmail" placeholder="Email">
						</div>
					</div>
					<div class="form-group">
						<label for="vendorAddress1" class="col-sm-2 control-label">Address</label>
						<div class="col-sm-10">
							<input type="text" class="form-control" name="address1" id="vendorAddress1" placeholder="Address">
						</div>
					</div>
					<div class="form-group">
						<label for="vendorAddress2" class="col-sm-2 control-label"></label>
						<div class="col-sm-10">
							<input type="text" class="form-control" name="address2" id="vendorAddress2" placeholder="">
						</div>
					</div>
					<div class="form-group">
						<label for="vendorCity" class="col-sm-2 control-label">City</label>
						<div class="col-sm-10">
							<input type="text" class="form-control" name="city" id="vendorCity" placeholder="City">
						</div>
					</div>
					<div class="form-group">
						<label for="vendorProvince" class="col-sm-2 control-label">State</label>
						<div class="col-sm-10">
							<input type="text" class="form-control" name="province" id="vendorProvince" placeholder="State">
						</div>
					</div>
					<div class="form-group">
						<label for="vendorZip" class="col-sm-2 control-label">Zip</label>
						<div class="col-sm-10">
							<input type="text" class="form-control" name="zip" id="vendorZip" placeholder="Zip">
						</div>
					</div>
					<div class="form-group">
						<label for="vendorCountry" class="col-sm-2 control-label">Country</label>
						<div class="col-sm-10">
							<input type="text" class="form-control" name="country" id="vendorCountry" placeholder="Country">
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-link" data-dismiss="modal">Cancel</button>
					<input type="submit" value="New Vendor" class="btn btn-success"></button>
				</div>
			</form>
		</div>
	</div>
</div>